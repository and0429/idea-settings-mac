<templateSet group="UserDefine">
  <template name="t" value="*&#10; * @Author: $user$&#10; * @Date: $date$      &#10; * @Time:  $time$&#10; *       &#10; * @Description $description$  &#10; */" shortcut="ENTER" description="Class Description" toReformat="false" toShortenFQNames="true">
    <variable name="user" expression="user()" defaultValue="zhangkai" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="description" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>